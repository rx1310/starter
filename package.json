{
	"version": "0.0.1",
	"scripts": {
    "start": "run-p 11ty:serve sass:watch",
    "build": "run-s sass:compile 11ty:build",
    "sass:compile": "sass --no-source-map app/styles/:dist/styles/ --style compressed",
    "sass:watch": "sass --no-source-map app/styles/:dist/styles/ --watch",
    "11ty:serve": "npx @11ty/eleventy --serve --config=.11ty.js",
    "11ty:build": "npx @11ty/eleventy --config=.11ty.js"
  },
	"devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@rx1310/kalium19": "^3.0.4",
    "js-yaml": "^4.1.0",
    "markdown-it": "^12.3.2",
    "markdown-it-attrs": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "sass": "^1.50.0"
  },
	"dependencies": {
    "@rx1310/grid-v4": "^4.4.2",
    "sanitize.css": "^13.0.0"
  }
}
